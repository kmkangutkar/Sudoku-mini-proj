#include<stdio.h>
#include<stdlib.h>
//#include "istack.h"
#include "pstack2.h"
#define ROW 9
#define	COL 9


int main(){
	int **puz;
	int *p, s, count = 0;
	int i, j, val, val1;
	istack w, t;
	char c;
	pstack POS;
	init(&POS);

// 0 denotes an empty cell

/*Tak\ning puzzle input*/
	puz = (int **)malloc(sizeof(int) * 9/*ROW*/);
	if(puz == NULL){
		printf("ERROR1\n");
		return 0;
	}
	for(i = 0; i < 9/*ROW*/; i++){
		puz[i] = (int *)malloc(sizeof(int) * 9/*COL*/);
		if(puz[i] == NULL){
			printf("ERROR2\n");
		}
	}
	printf("Made it till here: MALLOCED the memory\n");	
		
	for(i = 0; i < 9/*ROW*/; i++){
		for(j = 0; j < 9/*COL*/; j++){
			printf("take input %d %d\n", i, j);
			scanf("%d", &puz[i][j]);
		}	
	}
	
	for(i = 0; i < ROW; i++){
		for(j = 0; j < COL; j++){
			printf("%d ", puz[i][j]);
		}	
		printf("\n");	
	}
}
